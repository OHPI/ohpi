    <!-- http://purl.obolibrary.org/obo/OHPI_0000002 -->

    <owl:ObjectProperty rdf:about="&obo;OHPI_0000002">
        <rdfs:label xml:lang="en">gene mutant attenuated in host organism</rdfs:label>
        <obo:IAO_0000117>YL, YH</obo:IAO_0000117>
        <obo:IAO_0000115>an object property that represents a relation between a gene and an organism where the gene comes from a pathogen, and the organism is a host organism, and the mutant of the gene for the pathogen is attenuated in the host organism. </obo:IAO_0000115>
        <rdfs:range rdf:resource="&obo;OBI_0100026"/>
        <rdfs:domain rdf:resource="&obo;OGG_0000000002"/>
        <rdfs:subPropertyOf rdf:resource="&obo;OHPI_0000007"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/OHPI_0000003 -->

    <owl:ObjectProperty rdf:about="&obo;OHPI_0000003">
        <rdfs:label xml:lang="en">organism attenuated in host</rdfs:label>
        <obo:IAO_0000117>YH, YL</obo:IAO_0000117>
        <obo:IAO_0000115>a shortcut relation that shows a mutant is attenuated in an infected host (e.g., an organism, a cell) compared to its wild type. </obo:IAO_0000115>
        <rdfs:domain rdf:resource="&obo;OBI_0100026"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/OHPI_0000004 -->

    <owl:ObjectProperty rdf:about="&obo;OHPI_0000004">
        <rdfs:label xml:lang="en">organism attenuated in host organism</rdfs:label>
        <obo:IAO_0000115>a disposition that displays an attenuated feature of a mutant compared its wild type organism when the mutant or its wild type organism infects a host organism. </obo:IAO_0000115>
        <obo:IAO_0000117>YH, YL</obo:IAO_0000117>
        <rdfs:range rdf:resource="&obo;OBI_0100026"/>
        <rdfs:subPropertyOf rdf:resource="&obo;OHPI_0000003"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/OHPI_0000005 -->

    <owl:ObjectProperty rdf:about="&obo;OHPI_0000005">
        <rdfs:label xml:lang="en">gene mutant attenuated in host cell</rdfs:label>
        <obo:IAO_0000115>an object property that represents a relation between a gene and a cell where the gene comes from a pathogen, and the cell is a host organism cell, and the mutant of the gene for the pathogen is attenuated inside the cell. </obo:IAO_0000115>
        <obo:IAO_0000117>YL, YH</obo:IAO_0000117>
        <rdfs:range rdf:resource="&obo;CL_0000000"/>
        <rdfs:domain rdf:resource="&obo;OGG_0000000002"/>
        <rdfs:subPropertyOf rdf:resource="&obo;OHPI_0000007"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/OHPI_0000006 -->

    <owl:ObjectProperty rdf:about="&obo;OHPI_0000006">
        <rdfs:label xml:lang="en">organism attenuated in host cell</rdfs:label>
        <obo:IAO_0000117>YH, YL</obo:IAO_0000117>
        <obo:IAO_0000115>a disposition that displays an attenuated feature of a mutant compared its wild type organism when the mutant or its wild type organism infects a host cell. </obo:IAO_0000115>
        <rdfs:range rdf:resource="&obo;CL_0000000"/>
        <rdfs:subPropertyOf rdf:resource="&obo;OHPI_0000003"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/OHPI_0000007 -->

    <owl:ObjectProperty rdf:about="&obo;OHPI_0000007">
        <rdfs:label xml:lang="en">gene mutant attenuated in host</rdfs:label>
        <obo:IAO_0000115>an object property that represents a relation between a gene and a host where the gene comes from a pathogen, and the mutant of the gene for the pathogen is attenuated in the host. </obo:IAO_0000115>
        <obo:IAO_0000117>YL, YH</obo:IAO_0000117>
        <rdfs:domain rdf:resource="&obo;OGG_0000000002"/>
    </owl:ObjectProperty>
    


