        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a protocol specifying to administer 1 ml of vaccine to a mouse</obo:IAO_0000112>
        <obo:IAO_0000111 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dose specification</obo:IAO_0000111>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000123"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/obi.owl"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OBI_0000984 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0000984">
