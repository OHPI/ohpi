        <rdfs:seeAlso>Protegen ID: 477; NCBI Gene GI: 3191792; NCBI Protein GI: 56708002; NCBI Protein Accession: YP_169898; NCBITaxon: 177416</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/VO_0011084 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/VO_0011084">
